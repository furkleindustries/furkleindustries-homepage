{
  "name": "furkleindustries-homepage",
  "version": "0.1.0",
  "scripts": {
    "ng": "ng",
    "build:ssr": "npm run build:client-and-server-bundles && npm run webpack:server && npm run prerender",
    "serve:ssr": "cross-env h2='h2' npm run build:ssr && node dist/server/server",
    "build:client-and-server-bundles": "ng build --prod && ng run furkleindustries-homepage:server",
    "webpack:server": "webpack --config server/webpack.server.config.js --progress --colors",
    "prerender": "node dist/server/prerender",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test --watch=true --progress true",
    "test:headless": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "travis-tests": "npm run lint && npm run test:headless && npm run e2e",
    "ansible:provision": "cd ansible && ansible-playbook -i hosts provision.yml",
    "ansible:update": "cd ansible && ansible-playbook -i hosts update.yml"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.0.5",
    "@angular/common": "^6.0.5",
    "@angular/compiler": "^6.0.5",
    "@angular/core": "^6.0.5",
    "@angular/forms": "^6.0.5",
    "@angular/http": "^6.0.5",
    "@angular/platform-browser": "^6.0.5",
    "@angular/platform-browser-dynamic": "^6.0.5",
    "@angular/platform-server": "^6.0.6",
    "@angular/pwa": "^0.6.8",
    "@angular/router": "^6.0.5",
    "@nguniversal/express-engine": "^6.0.0",
    "@nguniversal/module-map-ngfactory-loader": "^6.0.0",
    "compression": "^1.7.2",
    "core-js": "^2.5.4",
    "express": "^4.16.3",
    "express-sslify": "^1.2.0",
    "express-static-gzip": "^0.3.2",
    "gulp-cli": "^2.0.1",
    "rxjs": "^6.2.1",
    "sass": "^1.6.1",
    "serve-favicon": "^2.5.0",
    "spdy": "^3.4.7",
    "ts-loader": "^4.4.1",
    "zone.js": "^0.8.26",
    "@angular/service-worker": "^6.0.5"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.6.8",
    "@angular/cli": "^6.0.8",
    "@angular/compiler-cli": "^6.0.5",
    "@angular/language-service": "^6.0.5",
    "@types/compression": "0.0.36",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.2.1",
    "cross-env": "^5.2.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.0",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.3.0",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1",
    "typescript": "~2.7.2",
    "webpack-cli": "^3.0.8"
  }
}
